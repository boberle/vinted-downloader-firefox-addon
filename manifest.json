    "downloads",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "tabs"
  ],
  "background": {
    "scripts": ["background/background.js"]
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "96": "icons/icon96.png"
  },
